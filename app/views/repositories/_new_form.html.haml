.col-md-6
  .panel.panel-info
    .panel-heading New Repository
    .panel-body 
      = form_for @repository, html: { multipart: true, class: 'form-horizontal', remote: true, id: 'new_form'}  do |f| 
        = render 'shared/error_messages', object: f.object
        = render :partial => 'form', :locals => {:f => f}